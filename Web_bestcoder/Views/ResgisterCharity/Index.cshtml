        .program-text-inner {
            margin-bottom: 20px; /* Space between text and price section */
        }

        /* Price section */
        .program-teacher {
            padding-top: 10px;
            padding-bottom: 10px;
        }

        /* Button section at the bottom */
        .program-item .d-flex {
            margin-top: auto;
        }

        .page-header2 {
            position: relative;
            padding: 0;
            overflow: hidden;
        }
        /* Hover effect for the h4 text */
        .program-text-inner a.h4 {
            color: inherit; /* Keeps the original color */
            text-decoration: none; /* Removes underline if present */
